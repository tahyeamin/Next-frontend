{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    // ЁЯСЗ ржПржЗ ржЕржВрж╢ржЯрзБржХрзБ ржЖрж╕рж▓ред ржЖржкржирж╛рж░ ржлрж╛ржЗрж▓рзЗ ржПржЗ paths ржЕржВрж╢ржЯрж┐ рж╣рзБржмрж╣рзБ ржХржкрж┐-ржкрзЗрж╕рзНржЯ ржХрж░рзБржи
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "src/middleware.backup"  ],
  "exclude": [
    "node_modules"
  ]
}
